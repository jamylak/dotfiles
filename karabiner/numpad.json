{
  "description": "Map numpad keys to Ctrl + number for switching macOS desktops",
  "manipulators": [
    {
      "type": "basic",
      "from": {
        "key_code": "keypad_0"
      },
      "to": [
        {
          "key_code": "spacebar",
          "modifiers": [
            "left_shift",
            "left_command"
          ]
        }
      ]
    },
    {
