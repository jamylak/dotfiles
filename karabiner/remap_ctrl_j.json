{
  "description": "ctrl-j to enter for some apps",
  "manipulators": [
    {
      "conditions": [
        {
          "bundle_identifiers": [
            "^com\\.google\\.Chrome$",
            "^com\\.apple\\.Safari$"
          ],
          "type": "frontmost_application_if"
        }
      ],
      "from": {
        "key_code": "j",
        "modifiers": {
          "mandatory": [
            "control"
          ],
          "optional": [
            "any"
          ]
        }
      },
      "to": [
        {
          "key_code": "return_or_enter"
        }
      ],
      "type": "basic"
    }
  ]
}
